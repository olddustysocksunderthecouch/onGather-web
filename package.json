{
  "name": "on-gather",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "help": "package-json-help.js",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix './src/**/*.{ts,tsx}'",
    "npm:audit": "npm run _npm:audit:spec",
    "storybook:build": "build-storybook -s public",
    "storybook:start": "start-storybook -p 9009 -s public",
    "reset": "npx rimraf node_modules && npm i",
    "test": "react-scripts test  --watchAll=false",
    "test:coverage": "npm test -- --coverage",
    "test:watch": "react-scripts test --env=jsdom --watchAll",
    "_eject": "react-scripts eject",
    "_git:pre-commit": "npm run lint && npm run test",
    "_git:pre-push": "",
    "_npm:audit:spec": "audit-ci --critical",
    "_test:ci": "CI=true react-scripts test"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.9",
